{
  "name": "presellcampaign",
  "version": "0.0.1",
  "description": "Ubiatarplay presell campaing",
  "main": "index.js",
  "scripts": {
    "compile": "rimraf build && ./node_modules/.bin/truffle compile",
    "test": "mocha test/Owned.js test/PreSell.js",
    "testPreSell": "mocha test/PreSell.js",
    "testOwned": "mocha test/Owned.js",
    "testrpc": "./node_modules/.bin/testrpc",
    "testSolidity": "./node_modules/.bin/truffle test",
    "test_all": "truffle test",
    "deploy": "truffle deploy",
    "console": "truffle console --network ganache",
    "migrateMain": "./node_modules/.bin/truffle migrate --network main",
    "migrateRopsten": "./node_modules/.bin/truffle.cmd migrate --network ropsten",
    "migrateGanache": "./node_modules/.bin/truffle migrate --network ganache",
    "win_deploy": "truffle.cmd deploy",
    "win_compile": "rimraf build && truffle.cmd compile",
    "win_testrpc": "./node_modules/.bin/testrpc.cmd",
    "win_test_all": "truffle.cmd test"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "ethereumjs-wallet": "^0.6.0",
    "truffle-contract": "2.0.5",
    "truffle-hdwallet-provider": "0.0.3",
    "web3": "^0.18.4",
    "web3-provider-engine": "^8.6.1",
    "zeppelin-solidity": "^1.3.0"
  },
  "devDependencies": {
    "bip39": "2.3.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "ethereumjs-testrpc": "2.2.7",
    "lodash": "^4.17.4",
    "mocha": "3.5.3",
    "rimraf": "^2.6.2",
    "truffle": "4.0.0-beta.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ubiatar/PreSellCampaign.git"
  },
  "keywords": [
    "ubiatar",
    "ubiatarplay",
